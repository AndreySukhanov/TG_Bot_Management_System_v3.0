{
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/index.py"
    },
    {
      "src": "/set_webhook",
      "dest": "/api/index.py"
    },
    {
      "src": "/webhook_info",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/dashboard(.*)",
      "dest": "/api/dashboard.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".:./utils:./handlers:./db:./nlp"
  },
  "regions": ["fra1"],
  "build": {
    "env": {
      "PYTHONPATH": ".:./utils:./handlers:./db:./nlp"
    }
  }
}