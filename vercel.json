{
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@4.1.0"
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/index.py"
    },
    {
      "src": "/set_webhook",
      "dest": "/api/index.py"
    },
    {
      "src": "/webhook_info",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "regions": ["fra1"],
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  }
}